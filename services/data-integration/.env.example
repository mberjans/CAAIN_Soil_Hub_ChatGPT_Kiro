# Data Integration Service Environment Variables

# Service Configuration
DATA_INTEGRATION_PORT=8003
LOG_LEVEL=INFO

# Weather API Keys
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Database Configuration (if needed for caching)
REDIS_URL=redis://localhost:6379
POSTGRES_URL=postgresql://user:password@localhost:5432/afas_db

# API Rate Limiting
WEATHER_API_RATE_LIMIT=1000
WEATHER_CACHE_TTL=300

# Service URLs (for inter-service communication)
QUESTION_ROUTER_URL=http://localhost:8000
RECOMMENDATION_ENGINE_URL=http://localhost:8001
AI_AGENT_URL=http://localhost:8002